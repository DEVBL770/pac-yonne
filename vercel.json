{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static-build", "config": { "distDir": "dist" } },
    { "src": "api/send.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "^/api/send$", "dest": "api/send.js" },
    { "src": "^/(.*)$", "dest": "/index.html" }
  ]
}